{
    "sync.autoUpload": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "todo-tree.tree.showScanModeButton": false,
    "cSpell.userWords": [
        "Bader",
        "Chell",
        "Glasswire",
        "Netlimiter",
        "Privatezilla",
        "Privazer",
        "Swiper",
        "Traceroute",
        "Ublock",
        "awadh",
        "backends",
        "deserialization",
        "during",
        "flexbox",
        "fullscreen",
        "icmp",
        "infile",
        "minmax",
        "numpy",
        "pango",
        "picom",
        "pinentry",
        "popup",
        "redteam",
        "statusline",
        "usermod",
        "vscodium",
        "whois",
        "xfsettingsd",
        "xrdb",
        "xresources"
    ],
    "workbench.colorTheme": "Nord",
    "[dockerfile]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "explorer.confirmDelete": false,
    "python.defaultInterpreterPath": "/usr/bin/python",
    "python.pythonPath": "C:\\Python39\\python.exe",
    "todo-tree.general.debug": true,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "editor.rulers": [72, 79],
    "workbench.iconTheme": "helium-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "workbench.editorAssociations": [],
    "editor.codeLensFontFamily": "Inter",
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "cSpell.enableFiletypes": [
        "csv"
    ],
    "html.format.wrapLineLength": 79,
    "comment-divider.languagesMap": {
        "Plain Text":   ["#", "#"]
    },
    "comment-divider.length": 79,
    "editor.semanticHighlighting.enabled": true,
    "window.menuBarVisibility": "compact",
    "window.zoomLevel": -1,
    "editor.fontSize": 16,
    "editor.inlineHints.fontFamily": " 'Fira Code'",
    "scm.inputFontSize": 14,
    "markdown.preview.fontSize": 16
}
